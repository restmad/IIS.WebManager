{
  "apps": [
    {
      "assets": [
        { "glob": "manifest.json", "input": "app/resources", "output": "" },
        { "glob": "**/*", "input": "app/assets/", "output": "assets/" }
      ],
      "index": "app/main/index.html",
      "main": "app/main/main.ts",
      "root": "",
      "scripts": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/bootstrap/dist/js/bootstrap.min.js"
      ],
      "styles": [
        "node_modules/primeng/resources/primeng.min.css",
        "node_modules/primeng/resources/themes/bootstrap/theme.css",
        "node_modules/@microsoft/windows-admin-center-sdk/dist/angular/assets/sme-icons/css/sme-icons.css",
        "node_modules/font-awesome/css/font-awesome.min.css",
        "node_modules/bootstrap/dist/css/bootstrap.min.css",
        "themes/main.css",
        "themes/themes.common.css",
        "themes/themes.blue.css"
      ],
      "tsconfig": "tsconfig.json"
    }
  ],
  "defaults": {
    "inline": {
      "style": false,
      "template": false
    },
    "prefixInterfaces": false,
    "styleExt": "css",
    "build": {
      "showCircularDependencies": true
    }
  },
  "project": {
    "name": "wac-iis",
    "version": "1.0.0-beta.001"
  }
}
